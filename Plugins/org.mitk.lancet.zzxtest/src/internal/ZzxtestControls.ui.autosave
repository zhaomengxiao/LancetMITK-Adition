      <string notr="true">/* TabWidget 的整体样式 */
QTabWidget::pane {
    border: 1px solid #CCCCCC;      /* 标签框架的边框颜色 */
    border-radius: 6px;             /* 标签框架的圆角 */
    padding: 1px;                   /* 标签内容的内边距 */
       /* 修改背景颜色为白色 */
}

/* TabBar 的样式 */
QTabBar::tab {
    background: #E8E8E8;            /* 未选中的选项卡背景颜色 */
    color: #333333;                 /* 未选中的选项卡文字颜色 */
    border: 1px solid #CCCCCC;      /* 选项卡的边框颜色 */
    border-radius: 2px;             /* 选项卡的圆角 */
    padding: 8px 16px;              /* 选项卡的内边距 */
    margin-right: 4px;              /* 选项卡之间的间距 */
}

/* 选中的选项卡 */
QTabBar::tab:selected {
    background: #FFFFFF;            /* 选中的选项卡背景颜色 */
    color: #1E90FF;                 /* 选中的选项卡文字颜色 */
    border-bottom-color: #FFFFFF;   /* 让选中的选项卡和内容背景一致 */
}

/* 鼠标悬停时的效果 */
QTabBar::tab:hover {
    background: #F0F0F0;            /* 鼠标悬停时的背景颜色 */
    color: #000000;                 /* 鼠标悬停时的文字颜色 */
}

/* 选项卡按钮（左右箭头）的样式 */
QTabBar QToolButton {
    background: #E8E8E8;            /* 按钮背景颜色 */
    border: 1px solid #CCCCCC;      /* 边框颜色 */
    border-radius: 6px;             /* 圆角 */
}

/* 左右按钮的悬停效果 */
QTabBar QToolButton:hover {
    background: #DCDCDC;            /* 鼠标悬停时的背景颜色 */
    color: #1E90FF;                 /* 鼠标悬停时的文字颜色 */
}
