QWidget
{
    background-color:#d4deea;
	border:none;
	color:black;
}

QFrame#line1
{
    border-image: url(:/org.mitk.lancet.pelvisprecisionregistrations/image/fenge.png);
}
QFrame#line2
{
    border-image: url(:/org.mitk.lancet.pelvisprecisionregistrations/image/fenge.png);
}

QLCDNumber
{
	color: rgb(66, 181, 158);
	border: 1px solid gray;
	border-image: url(:/org.mitk.lancet.pelvisprecisionregistrations/image/value.png);
	border-radius: 10px;
}

QProgressBar {
	/* 外边框 */
	border:1px solid black;
	
	/* 倒角 */
	border-radius:5px;
	
	background-image: url(:/org.mitk.lancet.pelvisprecisionregistrations/image/scrool.png);
	/* 字体对齐方式 */
	text-align:center;
}

/* 进度条 */
QProgressBar::chunk {
	/* 颜色 */
	background-color:rgb(0, 183, 138);
	
	/* 步进距离 */
	width:5px;
	
	/* 进度条中间空白区域宽度 */
	margin:1px;
}
QPushButton{
	border-image: url(:/org.mitk.lancet.pelvisprecisionregistrations/image/default.png);
	color:white;
}
QPushButton::hover
{
	border-image: url(:/org.mitk.lancet.pelvisprecisionregistrations/image/on.png);
	color:balck;
}
QPushButton::disabled
{
	border-image: url(:/org.mitk.lancet.pelvisprecisionregistrations/image/disabled.png);	
	color: rgb(153, 153, 153);
}
QPushButton::pressed { 
	padding-left:6px;
	padding-top:6px;
}